{
    "usesTypeScript": true,
    "usesCssModule": true,
    "cssPreprocessor": "scss",
    "testLibrary": "Testing Library",
    "component": {
        "default": {
            "customTemplates": {
                "component": "src/templates/default/TemplateName.tsx",
                "style": "src/templates/default/TemplateName.module.scss",
                "index": "src/templates/default/index.ts",
                "props": "src/templates/default/TemplateName.props.ts"
            },
            "path": "src/components",
            "withLazy": false,
            "withStory": false,
            "withStyle": true,
            "withTest": false,
            "withIndex": true,
            "withProps": true
        }
    }
}